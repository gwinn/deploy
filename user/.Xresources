Xft.dpi: 96
Xft.antialias: true
Xft.rgba: rgb
Xft.hinting: true
Xft.hintstyle: 1
Xft.autohint: false
Xft.lcdfilter: 1
!URxvt*termName: rxvt-unicode

URxvt.scrollstyle: plain
URxvt.scrollBar: false

URxvt*scrollWithBuffer: true
URxvt*scrollTtyKeypress: true
URxvt.font: xft:RobotoMono Nerd Font:size=9
URxvt.allow_bold: false
URxvt.letterSpace: 0
URxvt*saveLines: 10000
URxvt.cursorBlink: true
URxvt.cursorUnderline: true
!URxvt*skipBuiltinGlyphs: true

! Background color
URxvt*background: gray15

! Цвет шрифта
URxvt*foreground: white

! Другие цвета
URxvt*color0: black
URxvt*color1: red3
URxvt*color2: green3
URxvt*color3: yellow3
URxvt*color4: blue2
URxvt*color5: magenta3
URxvt*color6: cyan3
URxvt*color7: gray90
URxvt*color8: grey50
URxvt*color9: red
URxvt*color10: green
URxvt*color11: yellow
URxvt*color12: #81a2be
URxvt*color13: magenta
URxvt*color14: cyan
URxvt*color15: white

URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: /usr/bin/firefox
URxvt.url-underline: true
URxvt.matcher.button:  1
URxvt.colorUL : #81a2be
